def check_eligibility(math_marks, physics_marks, chemistry_marks):
    total_marks = math_marks + physics_marks + chemistry_marks
    if math_marks >= 65 and physics_marks >= 55 and chemistry_marks >= 50:
        return True
    elif total_marks >= 180:
        return True
    else:
        return False
math_marks = int(input())
physics_marks = int(input())
chemistry_marks = int(input())
if check_eligibility(math_marks, physics_marks, chemistry_marks):
    print("The candidate is eligible")
else:
    print("The candidate is not eligible")
