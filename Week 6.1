# Input
n = int(input())
A = list(map(int, input().split()))
B = list(map(int, input().split()))

# Create a dictionary to map elements of B to their indices
index_map = {val: i for i, val in enumerate(B)}

# Find the index mapping for each element in A
P = [index_map[val] for val in A]

# Output the result
print(*P)
