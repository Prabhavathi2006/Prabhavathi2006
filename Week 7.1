def eliminate_common_elements(arr1, arr2):
    # Convert the arrays to sets to find the common elements
    set1 = set(arr1)
    set2 = set(arr2)
    
    # Find the common elements
    common_elements = set1.intersection(set2)
    
    # Create lists of non-repeating elements
    unique_arr1 = [x for x in arr1 if x not in common_elements]
    unique_arr2 = [x for x in arr2 if x not in common_elements]
    
    # Combine the unique elements from both arrays
    unique_elements = unique_arr1 + unique_arr2
    
    if not unique_elements:
        print("NO SUCH ELEMENTS")
    else:
        print(" ".join(map(str, unique_elements)))
        print(len(unique_elements))

def main():
    # Read the input sizes of the arrays
    sizes = input().strip().split()
    size1 = int(sizes[0])
    size2 = int(sizes[1])
    
    # Read the arrays
    arr1 = list(map(int, input().strip().split()))
    arr2 = list(map(int, input().strip().split()))
    
    # Call the function to eliminate common elements
    eliminate_common_elements(arr1, arr2)

if __name__ == "__main__":
    main()
